<%= form_for list do |form| %>
  <%=label_nome %> :
  <%= form.text_field :name %><br />
  Tipo : <%= form.select :tipo, ["publica", "privada"], include_blank: true %><br /><br><br>
  <%= form.submit %>
<% end %>
<% if list.errors.any? %>
  <% list.errors.full_messages.each do |message| %>
    <%= message %>
  <% end %>
<% end %>
